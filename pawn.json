{
	"user": "Southclaws",
	"repo": "samp-sandbox",
	"entry": "gamemodes/sandbox.pwn",
	"output": "gamemodes/sandbox.amx",
	"dependencies": [
		"sampctl/samp-stdlib",
		"Southclaws/samp-logger",
		"Southclaws/pawn-errors",
		"pawn-lang/YSI-Includes",
		"ScavengeSurvive/settings",
		"Southclaws/pawn-requests:0.8.2",
		"lassir/bcrypt-samp",
		"Awsomedude/easyDialog",
		"BigETI/pawn-map"
	],
	"local": true,
	"builds": [
		{
			"name": "dev",
			"includes": [
				"gamemodes/sandbox/player-spawn",
				"gamemodes/sandbox/player-auth",
				"gamemodes/sandbox/class-selection"
			],
			"constants": {
				"NO_AUTH": "1"
			}
		},
		{
			"name": "prod",
			"includes": [
				"gamemodes/sandbox/player-spawn",
				"gamemodes/sandbox/player-auth",
				"gamemodes/sandbox/class-selection"
			]
		}
	],
	"runtime": {
		"version": "0.3.7",
		"mode": "server",
		"echo": "loading server.cfg generated by sampctl - do not edit this file manually, edit samp.json instead!",
		"rcon_password": "password",
		"port": 7777
	},
	"runtimes": [
		{
			"name": "main",
			"version": "0.3.7",
			"mode": "server",
			"port": 7777
		},
		{
			"name": "testing",
			"version": "0.3.7",
			"mode": "y_testing",
			"port": 7777
		}
	]
}