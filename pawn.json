	"runtimes": [
		{
			"name": "main",
			"version": "0.3.7",
			"mode": "server",
			"port": 7777
		},
		{
			"name": "testing",
			"version": "0.3.7",
			"mode": "y_testing",
			"port": 7777
		}
	]
