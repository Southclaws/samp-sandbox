			}
		},
		{
			"name": "dev-noauth",
			"includes": [
				"gamemodes/sandbox/utils",
				"gamemodes/sandbox/gamemode-core",
				"gamemodes/sandbox/world",
				"gamemodes/sandbox/item-spawns",
				"gamemodes/sandbox/player-lifecycle",
				"gamemodes/sandbox/player-auth",
				"gamemodes/sandbox/class-selection",
				"gamemodes/sandbox/dev-tools"
			],
			"constants": {
				"DEV_BUILD": "1",
				"NO_AUTH": "1"
			}
		},
		{
			"name": "prod",
			"includes": [
				"gamemodes/sandbox/player-lifecycle",
				"gamemodes/sandbox/player-auth",
				"gamemodes/sandbox/class-selection"
			]
		}
	],
	"runtime": {
		"version": "0.3.7",
